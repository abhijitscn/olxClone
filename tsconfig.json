{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext"
},
"filesGlob": [
  "typings/index.d.ts",
  "src/**/*.ts",
  "src/**/*.tsx",
  "node_modules/typescript/lib/lib.es6.d.ts"
],
"types": [
  "react",
  "react-dom",
  "react-native",
],
"exclude": [
  "build",
  "coverage",
  "node_modules",
  "babel.config.js",
  ".eslintrc.js",
  ".prettirerrc.js",
  "metro.config.js",
  "index.js",
],
"compileOnSave": false

}
